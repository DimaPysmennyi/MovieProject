model Movie{
    id             Int              @id @default(autoincrement())
    name           String 
    rating         Int
    age            String
    year           Int
    country        String
    language       String
    description    String           @default("")
    facts          String           @default("")
    previewSrc     String           @default("")
    shots          String           @default("")
    genres         MoviesOnGenres[] 
    reviews        Review[]
    actors         MoviesOnActors[]
}
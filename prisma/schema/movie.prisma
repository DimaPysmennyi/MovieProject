model Movie{
    id             Int              @id @default(autoincrement())
    name           String 
    rating         Float
    allRates       String           @default("")
    age            String
    year           Int
    country        String
    language       String
    description    String        
    facts          String           
    previewSrc     String           
    shots          String                  
    genres         MoviesOnGenres[] 
    reviews        Review[]
    actors         MoviesOnActors[]
}